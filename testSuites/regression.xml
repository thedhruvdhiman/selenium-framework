<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite">
  <listeners>
    <listener class-name="infinity.Test.Components.IListener"/>
  </listeners>
  <test thread-count="5" name="Complete order" parallel="methods">
    <classes>
      <class name="infinity.Test.OrderTests"/>
